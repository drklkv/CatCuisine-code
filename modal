<script type="text/javascript">
    const products = [
        {
            sku: "turkey",
            name: "Индейка",
            pricePerPack: 299,
            nutritionFacts: [
                "индейка, бедро: 48%",
                "кролик, филе: 41,4%",
                "цукини: 6,4%",
