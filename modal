                "питательные вещества: 3,8%",
            ],
            energy: "Обменная энергия: 132 ккал/100г",
            image: "https://i.ibb.co/kcLLg3p/image.png"
        },
        {
            sku: "nutria",
            name: "Нутрия",
            pricePerPack: 299,
            nutritionFacts: [
                "нутрия, филе: 89,2%",
                "огурец: 6,4%",
                "рыбий жир: 0,6%",
                "питательные вещества: 3,8%",
            ],
            energy: "Обменная энергия: 150 ккал/100г",
            image: "https://i.ibb.co/0tmjH7L/image.png"
        },
        {
            sku: "rabbit",
            name: "Кролик",
            pricePerPack: 299,
            nutritionFacts: [
                "кролик, филе: 89,2%",
                "болгарский перец: 6,4%",
                "рыбий жир: 0,7%",
                "питательные вещества: 3,8%",
            ],
            energy: "Обменная энергия: 143 ккал/100г",
            image: "https://i.ibb.co/q5NBMPL/image.png"
        },
        {
            sku: "veal",
            name: "Телятина",
            pricePerPack: 299,
            nutritionFacts: [
                "телятина, мясо: 73,2%",
                "сердце теленка: 19,1%",
                "морковь: 3,3%",
                "рыбий жир: 0,6%",
                "питательные вещества: 3,8%",
            ],
            energy: "Обменная энергия: 123 ккал/100г",
            image: "https://i.ibb.co/mC8XZVf/image.png"
        },
        {
            sku: "pink_salmon",
            name: "Горбуша",
            pricePerPack: 299,
            nutritionFacts: [
                "горбуша, филе: 89,2%",
                "тыква: 6,4%",
                "подсолнечное масло: 0,6%",
                "питательные вещества: 3,8%",
            ],
            energy: "Обменная энергия: 144 ккал/100г",
            image: "https://i.ibb.co/P9V4WG9/image.png"
        },
        {
            sku: "assorted",
            name: "Мясное ассорти",
            pricePerPack: 299,
            nutritionFacts: [
                "говяжий антрекот и сердце: 51%",
                "куриная грудка: 35%",
                "тыква: 9,5%",
                "рыбий жир: 0,7%",
                "питательные вещества: 3,8%",
            ],
            energy: "Обменная энергия: 134 ккал/100г",
            image: "https://i.ibb.co/tzdbZ90/image.png"
        },
    ];

    const findBySku = (name) => products.find(({sku}) => sku === name);

    window.ccPopup = {
        n: 1,
        discounts: [
            [7, 0.9665],
        ],
    };
    ccPopupUpdate = () => {
        const {n, discounts, target} = window.ccPopup;
        const {name, pricePerPack, energy, nutritionFacts, image} = findBySku(target);
        const total = n * pricePerPack;
        let discount = 1;
        for (const [q, d] of discounts) {
            if (n > q) discount = d;
        }
        document.getElementById("cc-modal-h2").textContent = name;
        document.getElementById("cc-modal-energy").textContent = energy;
        document.getElementById("cc-modal-image").src = image;
        document.getElementById("cc-modal-nutrition-facts").innerHTML = ''
        for (const fact of nutritionFacts) {
            const c = document.createElement('li');
            c.innerHTML = fact;
            document.getElementById("cc-modal-nutrition-facts").appendChild(c);
        }
        document.getElementById("cc-popup-n").value = n;
        document.getElementById("cc-popup-total").textContent = Math.round(
            n * pricePerPack * discount
        );
        document.getElementById("cc-popup-price-per-pack").textContent =
            Math.round(pricePerPack * discount);
        pricePerPack;
    };
    window.ccPopupInc = () => {
        window.ccPopup.n = window.ccPopup.n + 1;
        ccPopupUpdate();
    };
    window.ccPopupDec = () => {
        if (window.ccPopup.n > 1) {
            window.ccPopup.n = window.ccPopup.n - 1;
            ccPopupUpdate();
        }
    };
    window.ccPopup.submit = (e) => {
        const {name, pricePerPack, image, energy, nutritionFacts} = findBySku(window.ccPopup.target);
        let {n} = window.ccPopup;
        debugger;


        while (n) {
            window.tcart__addProduct({
                name,
                price: pricePerPack,
                img: image,
                url: "https://catcuisine.ru/",
            });
            n--;
        }
        window.ccPopup.n = 1;
        t868_closePopup(
            document.getElementById(
                document.getElementById("cc-modal").parentElement.parentElement
                    .parentElement.parentElement.parentElement.id
            )
        );
    };
    const oldPopupShow = window.t868_showPopup;
    window.t868_showPopup = function () {
        oldPopupShow(...arguments);
        window.ccPopupUpdate();
    };
    window.addEventListener("load", () => {
        document.getElementById("cc-modal-close").addEventListener("click", () => {
            t868_closePopup(
                document.getElementById(
                    document.getElementById("cc-modal").parentElement.parentElement
                        .parentElement.parentElement.parentElement.id
                )
            );
        });
        document.querySelectorAll('a').forEach(element => {
            if (element.href.indexOf('#item-details:') >= 0) {
                element.addEventListener("click", e => {
                    e.preventDefault();
                    window.ccPopup.target = element.href.match(/#item-details:(.*)/)[1]
                    window.t868_showPopup(document.getElementById(
                        document.getElementById("cc-modal").parentElement.parentElement
                            .parentElement.parentElement.parentElement.id
                    ));
                })
            }
        })
    });
    window.addTrialSet = function (event) {
        event.preventDefault();
        window.tcart__addProduct({
            name: "Welcome-сет (6 пакетов по 160 г)",
            price: 1690,
            img: 'https://i.postimg.cc/dVSVNvYf/probnii-nabor.webp',
            url: 'https://catcuisine.ru/'
        });
        return false;
    }
</script>
